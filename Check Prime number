#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int n;
 
    cin>>n;
   
    if(n%n==0 && n%1==0 &&n%2!=0)
    {
        cout<<"yes"<<endl;
    }
    
     else
    {
        cout<<"no"<<endl;
        
    }
    return 0;
}
